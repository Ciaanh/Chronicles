<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">

    <Frame name="MyJournalView" parent="MainFrame" hidden="true">
        <Size x="800" y="600"/>
        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>
        <Layers>
            <Layer level="ARTWORK">
                <FontString parentKey="Title" inherits="ChroniclesFontLarge" justifyH="CENTER">
                    <Size y="20"/>
                    <Anchors>
                        <Anchor point="TOP" x="0" y="-10"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Frame name="MyJournalViewList">
                <Size x="200" y="600"/>
                <Anchors>
                    <Anchor point="Left" />
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString parentKey="Title" inherits="ChroniclesFontLarge" justifyH="CENTER">
                            <Size y="20"/>
                            <Anchors>
                                <Anchor point="CENTER" x="0" y="0"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
            <Frame name="MyJournalViewDetails">
                <Size x="600" y="600"/>
                <Anchors>
                    <Anchor point="RIGHT" />
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <FontString parentKey="Title" inherits="ChroniclesFontLarge" justifyH="CENTER">
                            <Size y="20"/>
                            <Anchors>
                                <Anchor point="CENTER" x="0" y="0"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Frames>

                </Frames>
            </Frame>
        </Frames>
    </Frame>
</Ui>