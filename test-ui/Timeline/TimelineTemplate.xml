    <Script file="TimelineTemplate.lua"/>


    <Frame name="TimelineLabelTemplate" mixin="TimelineLabelMixin" virtual="true">
        <Size x="85" y="80"/>
        <Layers>
            <!-- <Layer level="BACKGROUND">
                <Texture parentKey="BlackBG" setAllPoints="true" alpha="0.5">
                    <Color r=".0" g="0.0" b="255.0"/>
                </Texture>
            </Layer> -->
            <Layer level="ARTWORK">
                <FontString parentKey="Text" inherits="SystemFont_Med3" setAllPoints="true" justifyH="CENTER">
                    <Anchors>
                        <Anchor point="TOPLEFT" />
                        <Anchor point="BOTTOMRIGHT"/>
                    </Anchors>
                    <Color color="SPELLBOOK_FONT_COLOR" />
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
    </Frame>

    <Button name="TimelinePeriodTemplate" mixin="TimelinePeriodMixin" virtual="true">
        <Size x="85" y="80"/>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="BlackBG" setAllPoints="true" alpha="0.5">
                    <Color r=".0" g="0.0" b="255.0"/>
                </Texture>
            </Layer>
            <Layer level="ARTWORK">
                <FontString parentKey="Text" inherits="ChroniclesFont" setAllPoints="true" justifyH="CENTER">
                    <Anchors>
                        <Anchor point="TOPLEFT" />
                        <Anchor point="BOTTOMRIGHT"/>
                    </Anchors>
                    <!-- <Color color="SPELLBOOK_FONT_COLOR" />-->
                    <!-- SystemFont_Med3 ChroniclesFont -->
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad method="OnLoad"/>
            <OnClick method="OnClick"/>
        </Scripts>
    </Button>


